<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.PlaylistMapper">
	<insert id="PlaylistAdd">
		insert into playlist(content_num,email,playlist_name)
		values(#{content_num},#{email},#{playlist_name})
	</insert>
	
	<select id="MyPlaylist" resultType="PlaylistDto">
	
	</select>
</mapper>